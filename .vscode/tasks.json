{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Escape Analysis",
      "type": "shell",
      "command": "go build -gcflags '-m -m'",
      "problemMatcher": {
        "pattern": {
          "regexp": "^(.*):(.*):(.*): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        },
        "fileLocation": "relative"
      }
    }
  ]
}
